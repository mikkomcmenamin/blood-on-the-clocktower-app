{
  "name": "botc-visualizer",
  "version": "1.0.0",
  "description": "Companion app for the game Blood on the Clocktower",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run be:dev\" \"npm run fe:dev\"",
    "start:prod": "concurrently \"npm run be:prod\" \"npm run fe:prod\"",
    "fe:dev": "cd frontend-web && vite",
    "fe:prod": "cd frontend-web && vite build && vite preview",
    "fe:build": "cd frontend-web && tsc && vite build",
    "fe:preview": "cd frontend-web && vite preview",
    "be:dev": "cd backend && nodemon src/server.ts",
    "be:prod": "cd backend && ts-node src/server.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikkomcmenamin/blood-on-the-clocktower-app.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mikkomcmenamin/blood-on-the-clocktower-app/issues"
  },
  "homepage": "https://github.com/mikkomcmenamin/blood-on-the-clocktower-app#readme",
  "dependencies": {
    "@trpc/client": "^10.6.0",
    "@trpc/server": "^10.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ws": "^8.11.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@types/node": "^18.11.17",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/ws": "^8.5.3",
    "@vitejs/plugin-react": "^3.0.0",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20",
    "sass": "^1.57.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  }
}
